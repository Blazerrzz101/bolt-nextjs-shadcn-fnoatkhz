{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    // Your custom rules
  },
  "linterOptions": {
    "reportUnusedDisableDirectives": true
  },
  "overrideConfig": {
    "linterOptions": {
      "reportUnusedDisableDirectives": "error"
    }
  }
}