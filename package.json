{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/pre-build-check.js && CI='' NODE_OPTIONS=\"--require=./scripts/minimal-entry.js\" next build",
    "build:vercel": "NODE_OPTIONS=\"--require=./scripts/minimal-entry.js\" next build",
    "build:mock": "MOCK_DB=true NODE_OPTIONS=\"--require=./scripts/minimal-entry.js\" next build",
    "start": "next start",
    "start:production": "NODE_OPTIONS=\"--require=./scripts/minimal-entry.js\" next start",
    "start:all": "./scripts/start-all.sh",
    "lint": "next lint",
    "deploy": "./scripts/deploy-to-production.sh",
    "deploy:preview": "vercel deploy",
    "health-check": "node scripts/health-check.js",
    "health-check:production": "node scripts/health-check.js https://tierd-app.vercel.app",
    "pre-build-check": "node scripts/pre-build-check.js",
    "deploy:simple": "./scripts/simple-deploy.sh",
    "monitor": "node scripts/system-monitor.js",
    "monitor:fast": "SYSTEM_MONITOR_INTERVAL=5000 node scripts/system-monitor.js",
    "stress-test": "node scripts/stress-test.js",
    "stress-test:light": "STRESS_TEST_USERS=10 STRESS_TEST_VOTES_PER_USER=5 node scripts/stress-test.js",
    "stress-test:heavy": "STRESS_TEST_USERS=100 STRESS_TEST_VOTES_PER_USER=20 STRESS_TEST_CONCURRENT_REQUESTS=20 node scripts/stress-test.js",
    "bot": "node scripts/activity-bot.js",
    "bot:light": "ACTIVITY_BOT_COUNT=2 ACTIVITY_BOT_INTERVAL=10000 node scripts/activity-bot.js",
    "bot:heavy": "ACTIVITY_BOT_COUNT=10 ACTIVITY_BOT_INTERVAL=1000 node scripts/activity-bot.js",
    "bot:timed": "ACTIVITY_BOT_COUNT=5 ACTIVITY_BOT_RUNTIME=300000 node scripts/activity-bot.js",
    "build:minimal": "node scripts/minimal-build.js",
    "build:enhanced": "NODE_OPTIONS=\"-r ./scripts/vercel-entry-enhanced.js\" next build",
    "start:enhanced": "NODE_OPTIONS=\"-r ./scripts/vercel-entry-enhanced.js\" next start",
    "dev:enhanced": "NODE_OPTIONS=\"-r ./scripts/vercel-entry-enhanced.js\" next dev",
    "build:safe": "CI='' NODE_OPTIONS=\"--require=./scripts/minimal-entry.js\" next build",
    "start:safe": "NODE_OPTIONS=\"--require=./scripts/minimal-entry.js\" next start",
    "vercel-build": "CI='' next build"
